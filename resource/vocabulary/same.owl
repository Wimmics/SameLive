<?xml version="1.0" encoding="utf-8" ?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:dcterms="http://purl.org/dc/terms/">

  <owl:Ontology rdf:about="https://ns.inria.fr/same/same.owl">
    <rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
    <rdfs:isDefinedby rdf:resource="https://ns.inria.fr/same/"/>
    <dcterms:creator>Raphael Gazzotti</dcterms:creator>
    <dcterms:creator>Fabien Gandon</dcterms:creator>
    <dcterms:title xml:lang="en">SameLive Vocabulary</dcterms:title>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-24-19</dcterms:modified>
    <vann:preferredNamespacePrefix>same</vann:preferredNamespacePrefix>
    <vann:preferredNamespaceUri>https://ns.inria.fr/same/</vann:preferredNamespaceUri>
    <dcterms:type rdf:resource="http://purl.org/adms/assettype/Ontology"/>
    <dcterms:status rdf:resource="http://purl.org/adms/status/UnderDevelopment"/>
  </owl:Ontology>

  <owl:Class rdf:about="https://ns.inria.fr/same/same.owl#Target">
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <rdfs:label>Targeted resource for discovering equivalence relationships.</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="https://ns.inria.fr/same/same.owl#Rotten">
    <rdf:type rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
    <rdfs:label>Resource that is source of potentially erroneous equivalence relationships.</rdfs:label>
  </owl:Class>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#hasIteration">
    <rdfs:domain rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    <rdfs:label>Iteration of the equivalence relationships algorithm during which the named graph was generated.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#hasNamespace">
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label>Namespace of the resource.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#hasAuthority">
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label>Authority of the resource.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#hasValueWithNoScheme">
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:label>String value of an URL without the scheme component.</rdfs:label>
  </owl:DatatypeProperty>

  <rdf:ObjectProperty rdf:about="https://ns.inria.fr/same/same.owl#hasSchemaFor">
    <rdfs:domain rdf:resource="http://rdfs.org/ns/void#Dataset"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>Specifies that the void:Dataset includes the resource's schemas.</rdfs:label>
  </rdf:ObjectProperty>

  <rdf:ObjectProperty rdf:about="https://ns.inria.fr/same/same.owl#statementInDataset">
    <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Statement"/>
    <rdfs:range rdf:resource="http://rdfs.org/ns/void#Dataset"/>
    <rdfs:label>Points to the void:Dataset that a statement is a part of.</rdfs:label>
  </rdf:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://ns.inria.fr/same/same.owl#votingType">
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label>rdf:type of the resource determined after voting.</rdfs:label>
  </owl:ObjectProperty>

  <rdf:Description rdf:about="https://ns.inria.fr/same/same.owl#N">
    <rdf:type rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:label>Named graph that contains information about the void:Dataset resources.</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="https://ns.inria.fr/same/same.owl#Properties">
    <rdf:type rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:label>Named graph that contains asserted owl:InverseFunctionalProperty and owl:FunctionalProperty.</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="https://ns.inria.fr/same/same.owl#PropertiesNotDeferenced">
    <rdf:type rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:label>Named graph that contains not deferenced properties after the LOAD clause.</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="https://ns.inria.fr/same/same.owl#Q-1">
    <rdf:type rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:label>Named graph that contains the same:Rotten resources.</rdfs:label>
  </rdf:Description>

  <rdf:Description rdf:about="https://ns.inria.fr/same/same.owl#Q0">
    <rdf:type rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:label>Named graph that contains the starting same:Target.</rdfs:label>
  </rdf:Description>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#triplesInGraph">
    <rdfs:domain rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of triples in the named graph.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#sameAsInGraph">
    <rdfs:domain rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of owl:sameAs triples in the named graph.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#targetsInGraph">
    <rdfs:domain rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of resources of type same:Target in the named graph.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#totalTargets">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of resources of type same:Target.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#totalRottens">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of resources of type same:Rotten.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#availableDatasets">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of datasets answering to HTTP requests.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#totalLoadedRDFDocuments">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of RDF documents loaded with the clause UPDATE LOAD.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#totalNotLoadedRDFDocuments">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of RDF documents not loaded with the clause UPDATE LOAD.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#totalRDFDocuments">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of RDF documents on which are defined resources.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#totalDeferencedInverseFunctionalProperties">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of owl:InverseFunctionalProperty properties for which it was not possible to identify schemas by loading their reference RDF document.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#totalNotDeferencedFunctionalProperties">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of owl:FunctionalProperty properties for which it was not possible to identify schemas by loading their reference RDF document.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#totalDeferencedFunctionalProperties">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of owl:FunctionalProperty properties for which it was possible to identify schemas by loading their reference RDF document.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#inNbOfDataset">
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of void:Dataset that a rdfs:Class is part of.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#nbOfTimesDefinedAsInverseFunctionalProperty">
    <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of time a property is defined as owl:InverseFunctionalProperty on void:Dataset that include the property's schema.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#nbOfTimesDefinedAsFunctionalProperty">
    <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Number of time a property is defined as owl:FunctionalProperty on void:Dataset that include the property's schema.</rdfs:label>
  </owl:DatatypeProperty>

  <rdf:Description rdf:about="https://ns.inria.fr/same/same.owl#S1">
    <rdf:type rdf:resource="https://w3id.org/function/ontology#Function"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    <dcterms:description>Retrieves owl:sameAs relationships.</dcterms:description>
  </rdf:Description>

  <rdf:Description rdf:about="https://ns.inria.fr/same/same.owl#IFP2">
    <rdf:type rdf:resource="https://w3id.org/function/ontology#Function"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/prov#Entity"/>
    <dcterms:description>Computes owl:sameAs relationships with (inverse) functional properties patterns.</dcterms:description>
  </rdf:Description>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#valuesIsAvailable">
    <rdfs:domain rdf:resource="http://labs.mondeca.com/vocab/endpointStatus#EndpointStatus"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:label>Describes whether a void:Dataset endpoint support the VALUES clause or not.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#supportsNonASCIICharacters">
    <rdfs:domain rdf:resource="http://labs.mondeca.com/vocab/endpointStatus#EndpointStatus"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:label>Describes whether a void:Dataset endpoint support non-ASCII characters or not.</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://ns.inria.fr/same/same.owl#hasResultsLimit">
    <rdfs:domain rdf:resource="http://labs.mondeca.com/vocab/endpointStatus#EndpointStatus"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <rdfs:label>Results limit returned by an endpoint.</rdfs:label>
  </owl:DatatypeProperty>

</rdf:RDF>
